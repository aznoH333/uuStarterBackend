  "paths": {
    "/logging/": {
      "get": {
        "description": "Returns all logs",
        "responses": {
          "200": {
            "description": "Returns a list of all logs in the application",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/LogEntry"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/projects/": {
      "get": {
        "summary": "Retrieve all projects",
        "responses": {
          "200": {
            "description": "A list of projects",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Project"
                  }
                }
              }
            }
          }
        },
        "tags": [
          "Projects"
        ]
      },
      "post": {
        "summary": "Create a new project",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  },
                  "ownerId": {
                    "type": "string"
                  },
                  "goalAmount": {
                    "type": "integer"
                  },
                  "deadLine": {
                    "type": "string",
                    "format": "date-time",
                    "example": "2025-11-05T14:51:54.670Z"
                  },
                  "categoryId": {
                    "type": "string",
                    "nullable": true
                  }
                },
                "required": [
                  "name",
                  "description",
                  "ownerId",
                  "goalAmount",
                  "deadLine"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Project created successfully"
          },
          "400": {
            "description": "Bad request, failed to create project"
          }
        },
        "tags": [
          "Projects"
        ]
      }
    },
    "/projects/{projectId}/": {
      "get": {
        "summary": "Retrieve a specific project by ID",
        "parameters": [
          {
            "name": "projectId",
            "in": "path",
            "required": true,
            "description": "The ID of the project to retrieve",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "A project object",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Project"
                }
              }
            }
          },
          "400": {
            "description": "Bad request, project not found"
          }
        },
        "tags": [
          "Projects"
        ]
      },
      "post": {
        "summary": "Update an existing project",
        "parameters": [
          {
            "name": "projectId",
            "in": "path",
            "required": true,
            "description": "The ID of the project to update",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  },
                  "goalAmount": {
                    "type": "integer"
                  },
                  "deadLine": {
                    "type": "string",
                    "format": "date-time"
                  },
                  "categoryId": {
                    "type": "string",
                    "nullable": true
                  },
                  "status": {
                    "type": "string",
                    "enum": [
                      "PendingApproval",
                      "Approved",
                      "Rejected",
                      "Closed"
                    ]
                  }
                },
                "required": [
                  "name",
                  "description",
                  "goalAmount",
                  "deadLine",
                  "status"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Project updated successfully"
          },
          "400": {
            "description": "Bad request, project not found or failed to update"
          }
        },
        "tags": [
          "Projects"
        ]
      }
    },
