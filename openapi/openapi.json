  "components": {
    "schemas": {
      "LogEntry": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "example": "690b6671afadc5771a921d81"
          },
          "logMessage": {
            "type": "string",
            "example": "Failed to create a project : ..."
          },
          "logType": {
            "type": "string",
            "enum": ["ERROR", "INFO", "DEBUG"]
          },
          "time": {
            "type": "string",
            "format": "date-time",
            "example": "2025-11-05T15:00:01.508Z"
          }
        },
        "required": ["_id", "logMessage", "logType", "time"]
      },
      "Project": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "example": "690b6710c001cd0566cf85a8"
          },
          "name": {
            "type": "string",
            "example": "Debug project"
          },
          "description": {
            "type": "string",
            "example": "Hello world"
          },
          "ownerId": {
            "type": "string"
          },
          "creationDate": {
            "type": "string",
            "format": "date-time",
            "example": "2025-11-05T15:02:40.949Z"
          },
          "lastUpdatedDate": {
            "type": "string",
            "format": "date-time",
            "example": "2025-11-05T15:02:40.949Z"
          },
          "goalAmount": {
            "type": "integer",
            "example": 20
          },
          "deadLine": {
            "type": "string",
            "format": "date-time",
            "example": "2025-11-05T14:51:54.670Z"
          },
          "categoryId": {
            "type": "string"
          },
          "status": {
            "type": "string",
            "enum": [
              "PendingApproval",
              "Approved",
              "Rejected",
              "Closed"
            ]
          }
        },
        "required": [
          "_id",
          "name",
          "description",
          "ownerId",
          "creationDate",
          "lastUpdatedDate",
          "goalAmount",
          "deadLine",
          "status"
        ]
      },
      "ProjectUpdate": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "example": "60ccf5d1f1d3f14b1c97b3a8"
          },
          "name": {
            "type": "string",
            "example": "Project Update"
          },
          "projectId": {
            "type": "string",
            "example": "690b6710c001cd0566cf85a8"
          },
          "content": {
            "type": "string",
            "example": "This is an update regarding the project."
          },
          "creationDate": {
            "type": "string",
            "format": "date-time",
            "example": "2025-11-05T15:02:40.949Z"
          },
          "lastUpdatedDate": {
            "type": "string",
            "format": "date-time",
            "example": "2025-11-05T15:02:40.949Z"
          }
        },
        "required": [
          "_id",
          "name",
          "projectId",
          "content",
          "creationDate",
          "lastUpdatedDate"
        ]
      },
      "ProjectComment": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "example": "60ccf5d1f1d3f14b1c97b3a8"
          },
          "authorId": {
            "type": "string",
            "example": "60ccf5d1f1d3f14b1c97b3a8"
          },
          "projectId": {
            "type": "string",
            "example": "690b6710c001cd0566cf85a8"
          },
          "content": {
            "type": "string",
            "example": "This project is going great!"
          },
          "creationDate": {
            "type": "string",
            "format": "date-time",
            "example": "2025-11-05T15:02:40.949Z"
          }
        },
        "required": [
          "_id",
          "authorId",
          "projectId",
          "content",
          "creationDate"
        ]
      },
      "ProjectRating": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "example": "60ccf5d1f1d3f14b1c97b3b9"
          },
          "userId": {
            "type": "string",
            "example": "690b6710c001cd0566cf85a8"
          },
          "projectId": {
            "type": "string",
            "example": "690b6710c001cd0566cf85a8"
          },
          "value": {
            "type": "number",
            "example": 4.5
          },
          "creationDate": {
            "type": "string",
            "format": "date-time",
            "example": "2025-11-05T15:02:40.949Z"
          }
        },
        "required": [
          "_id",
          "userId",
          "projectId",
          "value",
          "creationDate"
        ]
      }
    }
